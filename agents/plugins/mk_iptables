# iptables
if type iptables-save > /dev/null
then
        echo "<<<iptables>>>"
        # output filter configuration without table name, comments and
        # status data, i.e. lines beginning with '*', '#' or ':'.
        iptables-save -t filter | sed '/^[#*:]/d'
fi

