module(
    name = "clickhouse",
    # We want to use the same version that is used by the Cloud edition.
    # At the moment, the Cloud edition uses 25.3 from Altinity: https://builds.altinity.cloud/#altinitystable.
    # However, Cloud would switch to 25.8 if available in time.
    # Therefore, we limit ourselves to 25.8 from ClickHouse to avoid using features that would for sure be unavailable.
    version = "v25.8.11.66-lts",
    compatibility_level = 1,
)

bazel_dep(name = "bazel_skylib", version = "1.7.1")
