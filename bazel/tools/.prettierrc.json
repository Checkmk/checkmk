{
  "$schema": "https://json.schemastore.org/prettierrc",
  "semi": false,
  "tabWidth": 2,
  "singleQuote": true,
  "printWidth": 100,
  "trailingComma": "none",
  "plugins": ["./node_modules/@trivago/prettier-plugin-sort-imports/lib/src/index.js"],
  "importOrder": [
    "^[a-zA-z1-9@]{2, }$",
    "^@\/lib.*$",
    "^@\/components.*$",
    "^@\/form.*$",
    "^@\/.*$",
    "^[./]"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}
