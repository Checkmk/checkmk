usage() {
    cat <<HERE >&2
Used for internal monitoring of mknotifyd.
HERE
    return 1
}

echo '<<<mknotifyd:sep(0)>>>'
statefile=~/var/log/mknotifyd.state
[ -e "${statefile}" ] && {
    echo "[$(whoami)]"
    date +%s
    grep -v '^#' <"${statefile}"
}
