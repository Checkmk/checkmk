def agent_fuse_management_central(params: dict, hostname: str, ipaddress):
    args: list = [
        params["user"],
        passwordstore_get_cmdline("%s", params["password"]),
        params["url"],
        hostname,
    ]

    return args


special_agent_info['fuse_management_central'] = agent_fuse_management_central
