def agent_netapp_arguments(params, hostname, ipaddress):
    return [
        ipaddress,
        params["username"],
        params["password"],
        "--no_counters",
    ] + [element[4:] for element in params["skip_elements"] if element.startswith("ctr_")]


special_agent_info['netapp'] = agent_netapp_arguments
