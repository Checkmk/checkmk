#   |                                                  |___/               |
#   '----------------------------------------------------------------------'


check_info["raritan_px_sensors.humidity"] = {
    'inventory_function'        : lambda parsed: inventory_raritan_sensors(parsed, "humidity"),
    'check_function'            : check_raritan_sensors,
    'service_description'       : 'Humidity %s',
    'has_perfdata'              : True,
}


