from cmk.base.check_legacy_includes.tcp_connections import *  # pylint: disable=wildcard-import,unused-wildcard-import
# <<<tcp_conn_stats>>>
# 01 29
# 02 3
# 0A 26
# 05 1
# 06 187

# <<<tcp_conn_stats>>>
# LISTEN 39
# IDLE 3
# TIME_WAIT 1
# ESTABLISHED 68
# BOUND 1


def parse_tcp_conn_stats(info):
    parsed = empty_stats()
    for line in info:
        tcp_state = line[0]
        if len(tcp_state) == 2:
            tcp_state = map_counter_keys.get(int(tcp_state, 16))  # Hex
        if tcp_state is None:
            continue
        try:
            parsed[tcp_state] = int(line[1])
        except ValueError:
            pass
    return parsed


check_info["tcp_conn_stats"] = {
    'parse_function': parse_tcp_conn_stats,
    'inventory_function': inventory_tcp_connections,
    'check_function': check_tcp_connections,
    'service_description': 'TCP Connections',
    'has_perfdata': True,
    'group': 'tcp_conn_stats',
}
