def ups_generic_scan_function(oid):
    sys_obj_id = oid(".1.3.6.1.2.1.1.2.0")
    if sys_obj_id in [
            '.1.3.6.1.4.1.232.165.3',
            '.1.3.6.1.4.1.476.1.42',
            '.1.3.6.1.4.1.534.1',
            '.1.3.6.1.4.1.935',
            '.1.3.6.1.4.1.8072.3.2.10',
    ]:
        return True
    for oid_prefix in [
            '.1.3.6.1.2.1.33',
            '.1.3.6.1.4.1.5491',
            '.1.3.6.1.4.1.705.1',
            '.1.3.6.1.4.1.818.1.100.1',
    ]:
        if sys_obj_id.startswith(oid_prefix):
            return True
    return False


def discovery_ups_generic(info, default_levels_name):
    return [(idx, default_levels_name)
            for idx, raw_voltage, _raw_value in info
            if raw_voltage and int(raw_voltage)]
