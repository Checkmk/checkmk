cd $OMD_ROOT
for h in $(
    cd tmp/check_mk/piggyback
    echo *
); do
    lq "GET hosts\nColumns: address name\nFilter: address = $h\nFilter: name = $h\nOr: 2" | grep -q . || echo "$h"
done
