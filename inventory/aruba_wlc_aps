        "15": "NesotMW1700",
        "16": "ortronics Wi Jack Duo",
        "17": "ortronics Duo",
        "18": "ap80MB",
        "19": "ap80SB",
        "20": "ap85",
        "21": "ap124",
        "22": "ap125",
        "23": "ap120",
        "24": "ap121",
        "25": "ap1250",
        "26": "ap120abg",
        "27": "ap121abg",
        "28": "ap124abg",
        "29": "ap125abg",
        "30": "rap5wn",
        "31": "rap5",
        "32": "rap2wg",
        "33": "reserved-4",
        "34": "ap105",
        "35": "ap65wb",
        "36": "ap651",
        "37": "reserved-6",
        "38": "ap60p",
        "39": "reserved-7",
        "40": "ap92",
        "41": "ap93",
        "42": "ap68",
        "43": "ap68p",
        "44": "ap175p",
