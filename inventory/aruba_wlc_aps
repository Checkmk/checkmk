        "46": "ap175dc",
        "47": "ap134",
        "48": "ap135",
        "49": "reserved-8",
        "50": "ap93h",
        "51": "rap3wn",
        "52": "rap3wnp",
        "53": "ap104",
        "54": "rap155",
        "55": "rap155p",
        "56": "rap108",
        "57": "rap109",
        "58": "ap224",
        "59": "ap225",
        "60": "ap114",
        "61": "ap115",
        "62": "rap109L",
        "63": "ap274",
