from cmk.base.check_legacy_includes.k8s import *  # pylint: disable=wildcard-import,unused-wildcard-import


def inv_k8s_endpoints(info, inventory_tree, status_data_tree):
    parsed = parse_k8s(info)
    if not parsed["subsets"]:
        return
    inventory_tree.get_dict("software.applications.kubernetes.endpoints.").update(parsed)


inv_info['k8s_endpoint_info'] = {
    'inv_function': inv_k8s_endpoints,
}
