# definitions:
# cmp', '(obp-device = pyhsical CPU's
# core', '(core      = cores
# cpu', '(cpu        = thread


def inv_solaris_prtpicl(info):
    node = inv_tree("hardware.cpu.")
    cmp_no = 0
    core_no = 0
    cpu_no = 0
    for line in info:
        if line[0] == "cmp":
            cmp_no += 1
        elif line[0] == "core":
            core_no += 1
        elif line[0] == "cpu":
            cpu_no += 1

    node["cpus"] = cmp_no
    node["cores"] = core_no
    node["threads"] = cpu_no


inv_info["solaris_prtpicl"] = {
    "inv_function": inv_solaris_prtpicl,
}
