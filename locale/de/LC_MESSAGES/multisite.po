msgid "Hide this %s in dropdown menus"
msgstr "%s in der Dropdown-Liste ausblenden"

#, python-format
msgid "Hide this %s in the monitor menu"
msgstr "%s im Monitor-Menü ausblenden"

msgid "Hide user rules"
msgstr "Benutzerregeln zu"

msgid "Hierarchical layout"
msgstr "Hierarchisches Layout"

msgid "High"
msgstr "Hoch"

msgid "High (with escalation)"
msgstr "Hoch (mit Eskalation)"

msgid "High utilization at "
msgstr "Schwelle für hohe Auslastung bei"

msgid "High: Push notification alerts bypass quiet hours"
msgstr "Hoch: Alarme umgehen die definierten Ruhezeiten"

msgid "Historic Event Console Event"
msgstr "Historisches Ereignis der Event Console"

msgid "Historic Event Console Events"
msgstr "Historische Ereignisse der Event Console"

msgid "Historic messages"
msgstr "Historische Meldungen"

msgid "History"
msgstr "Verlauf"

msgid "History Line Number"
msgstr "Zeilennummer in der Historie"

msgid "History entries of one specific event"
msgstr "Historie eines bestimmten Events"

msgid "History entries of one specific host"
msgstr "Historie eines bestimmten Hosts"

msgid "History log rotation: Regular interval of rotations"
msgstr "Rotation der Historie: Regelmäßige Rotation"

msgid "History log rotation: Rotate by size (Limit of the size)"
