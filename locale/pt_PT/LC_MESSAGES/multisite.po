msgstr "Inativo (anónimo)"

msgid "Inactive (files)"
msgstr "Inativos (arquivos)"

msgid "Inactive hosts"
msgstr "Hospedeiros inativos"

msgid "Inactive sessions"
msgstr "Sessões inactivas"

