#!/bin/bash
# Call pdftoppm without LD_LIBRARY_PATH set, such that the system openssl is used

for pdftoppm_exec in $(type -ap pdftoppm); do
        if [ "$pdftoppm_exec" != "${BASH_SOURCE[0]}" ]; then
                unset LD_LIBRARY_PATH
                exec "$pdftoppm_exec" "$@"
        fi
done

echo "No pdftoppm executable other than ${BASH_SOURCE[0]} found, aborting" 1>&2
exit 1
