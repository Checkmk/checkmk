{
    "name": "check_mk",
    "version": "1.0.0",
    "description": "Checkmk - Your complete IT monitoring solution",
    "engines": {
        "npm": ">=10.9.0 <11.0.0",
        "node": ">=22.0.0 <=23.0.0"
    },
    "browserslist": [
        "last 2 version",
        "> 1%",
        "not dead",
        "not op_mini all"
    ],
    "scripts": {
        "build": "webpack",
        "build-watch": "npx webpack --mode development --watch",
        "test": "python3 scripts/test_scss_content.py",
        "test-eslint": "eslint --max-warnings 0 --no-color './src/js/**/*.{j,t}s'",
        "test-tsc": "tsc",
        "test-prettier": "prettier  --check --no-color --ignore-path ./.prettierignore --log-level warn '{src/**/,}*.{js,ts,scss}'",
        "prettier": "prettier  --write --no-color --ignore-path ./.prettierignore --log-level warn '{src/**/,}*.{js,ts,scss}'"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/Checkmk/checkmk"
    },
    "license": "GPL-2.0",
    "jest": {
        "testEnvironment": "jsdom",
        "modulePaths": [
            "<rootDir>/src/js/modules"
        ],
        "setupFiles": [
            "<rootDir>/src/js/tests/globals.mock.js",
            "fake-indexeddb/auto"
        ],
        "verbose": true
    },
    "devDependencies": {
        "@babel/core": "^7.26.0",
        "@babel/eslint-parser": "^7.25.9",
        "@babel/plugin-proposal-class-properties": "^7.18.6",
        "@babel/plugin-proposal-object-rest-spread": "^7.20.7",
        "@babel/plugin-transform-parameters": "^7.27.1",
        "@babel/preset-env": "^7.26.0",
        "@babel/preset-typescript": "^7.26.0",
        "@jest/globals": "^29.7.0",
        "@types/crossfilter": "^0.0.38",
        "@types/d3": "^7.4.3",
        "@types/d3-flextree": "^2.1.4",
        "@types/d3-hexbin": "^0.2.5",
        "@types/d3-sankey": "^0.12.4",
        "@types/jquery": "^3.5.32",
        "@types/node": "^22.10.1",
        "@types/qrcode": "^1.5.5",
        "@types/select2": "^4.0.63",
        "@types/yaireo__tagify": "^4.27.0",
        "@typescript-eslint/eslint-plugin": "^8.17.0",
        "@typescript-eslint/parser": "^8.17.0",
        "autoprefixer": "^10.4.20",
        "babel-loader": "^9.2.1",
        "browserslist": "^4.24.2",
        "caniuse-lite": "^1.0.30001686",
        "css-loader": "^7.1.2",
        "cssnano": "^7.0.6",
        "eslint": "^9.16.0",
        "eslint-config-prettier": "^9.1.0",
        "eslint-import-resolver-typescript": "^3.7.0",
        "eslint-plugin-compat": "^6.0.1",
        "eslint-plugin-import": "^2.31.0",
        "eslint-plugin-license-header": "^0.6.1",
        "eslint-plugin-no-unsanitized": "^4.1.2",
        "eslint-plugin-prettier": "^5.2.1",
        "eslint-plugin-simple-import-sort": "^12.1.1",
        "extract-loader": "^5.1.0",
        "fake-indexeddb": "^6.0.0",
        "file-loader": "^6.2.0",
        "filemanager-webpack-plugin": "^8.0.0",
        "jest-cli": "^29.7.0",
        "jest-environment-jsdom": "^29.7.0",
        "mini-css-extract-plugin": "^2.9.2",
        "mutation-observer": "^1.0.3",
        "npm-groovy-lint": "<15.0.0",
        "pkg-dir": "^8.0.0",
        "postcss": "^8.4.49",
        "postcss-loader": "^8.1.1",
        "prettier": "^3.4.2",
        "sass": "^1.82.0",
        "sass-loader": "^16.0.4",
        "script-loader": "^0.7.2",
        "style-loader": "^4.0.0",
        "terser-webpack-plugin": "^5.3.10",
        "ts-jest": "^29.2.5",
        "typescript": "^5.7.2",
        "webpack": "^5.97.0",
