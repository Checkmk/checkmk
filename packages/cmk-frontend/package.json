        "core-js": "^3.39.0",
        "crossfilter2": "^1.5.4",
        "d3": "^7.9.0",
        "d3-flextree": "^2.1.2",
        "d3-hexbin": "^0.2.2",
        "d3-sankey": "^0.12.3",
        "element-closest-polyfill": "^1.0.7",
