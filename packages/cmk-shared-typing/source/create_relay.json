{
  "$schema": "https://json-schema.org/draft/2019-09/schema",
  "type": "object",
  "title": "Create relay",
  "properties": {
    "alias_validation": {
      "type": "object",
      "properties": {
        "regex": {
          "type": "string"
        },
        "regex_help": {
          "type": "string"
        }
      },
      "required": ["regex", "regex_help"]
    },
    "urls": {
      "type": "object",
      "properties": {
        "create_host": {
          "type": "string"
        },
        "relay_overview": {
          "type": "string"
        },
        "automation_secret": {
          "type": "string"
        }
      },
      "required": ["create_host", "relay_overview", "automation_secret"]
    },
    "site_name": {
      "type": "string"
    },
    "domain": {
      "type": "string"
    },
    "site_version": {
      "type": "string"
    },
    "is_cloud_edition": {
      "type": "boolean"
    },
    "user_id": {
      "type": "string"
    }
  },
  "required": [
    "alias_validation",
    "urls",
    "site_name",
    "domain",
    "site_version",
    "is_cloud_edition",
    "user_id"
  ]
}
