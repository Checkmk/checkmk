{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "DynamicIconAppProps",
  "type": "object",
  "properties": {
    "spec": {
      "$ref": "#/$defs/dynamic_icon"
    }
  },
  "required": ["spec"],
  "$defs": {
    "dynamic_icon": {
      "title": "DynamicIcon",
      "type": "object",
      "oneOf": [
        {
          "$ref": "#/$defs/user_icon"
        },
        {
          "$ref": "#/$defs/default_icon"
        },
        {
          "$ref": "#/$defs/emblem_icon"
        }
      ]
    },
    "user_icon": {
      "title": "UserIcon",
      "type": "object",
      "properties": {
        "type": {
          "const": "user_icon",
          "default": "user_icon"
        },
        "id": {
          "type": "string"
        },
        "path": {
          "type": "string"
        }
      },
      "required": ["type", "id", "path"]
    },
    "default_icon": {
      "title": "DefaultIcon",
      "type": "object",
      "properties": {
        "type": {
          "const": "default_icon",
          "default": "default_icon"
        },
        "id": {
          "type": "string"
        }
      },
      "required": ["type", "id"]
    },
    "emblem_icon": {
      "title": "EmblemIcon",
      "type": "object",
      "properties": {
        "type": {
          "const": "emblem_icon",
          "default": "emblem_icon"
        },
        "icon": {
          "type": "object",
          "oneOf": [
            {
              "$ref": "#/$defs/user_icon"
            },
            {
              "$ref": "#/$defs/default_icon"
            }
          ]
        },
        "emblem": {
          "type": "string"
        }
      },
      "required": ["type", "icon", "emblem"]
    }
  }
}
