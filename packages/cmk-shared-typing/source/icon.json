{
  "$schema": "https://json-schema.org/draft/2020-12/schema",
  "title": "__Dummy__",
  "type": "object",
  "properties": {
    "dynamic_icon_app_props": {
      "$ref": "#/$defs/dynamic_icon_app_props"
    },
    "icon": {
      "$ref": "#/$defs/icon_names"
    },
    "size": {
      "$ref": "#/$defs/icon_sizes"
    }
  },
  "$defs": {
    "dynamic_icon_app_props": {
      "title": "DynamicIconAppProps",
      "type": "object",
      "properties": {
        "spec": {
          "$ref": "#/$defs/dynamic_icon"
        }
      },
      "required": ["spec"]
    },
    "dynamic_icon": {
      "title": "DynamicIcon",
      "type": "object",
      "oneOf": [
        {
          "$ref": "#/$defs/user_icon"
        },
        {
          "$ref": "#/$defs/default_icon"
        },
        {
          "$ref": "#/$defs/emblem_icon"
        }
      ]
    },
    "user_icon": {
      "title": "UserIcon",
      "type": "object",
      "properties": {
        "type": {
          "const": "user_icon",
          "default": "user_icon"
        },
        "id": {
          "type": "string"
        },
        "path": {
          "type": "string"
        }
      },
      "required": ["type", "id", "path"]
    },
    "default_icon": {
      "title": "DefaultIcon",
      "type": "object",
      "properties": {
        "type": {
          "const": "default_icon",
          "default": "default_icon"
        },
        "id": {
          "type": "string"
        }
      },
      "required": ["type", "id"]
    },
    "emblem_icon": {
      "title": "EmblemIcon",
      "type": "object",
      "properties": {
        "type": {
          "const": "emblem_icon",
          "default": "emblem_icon"
        },
        "icon": {
          "type": "object",
          "oneOf": [
            {
              "$ref": "#/$defs/user_icon"
            },
            {
              "$ref": "#/$defs/default_icon"
            }
          ]
        },
        "emblem": {
          "type": "string"
        }
      },
      "required": ["type", "icon", "emblem"]
    },
    "icon_sizes": {
      "type": "string",
      "enum": ["xxsmall", "xsmall", "small", "medium", "large", "xlarge", "xxlarge", "xxxlarge"]
