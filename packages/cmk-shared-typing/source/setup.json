{
    "$schema": "https://json-schema.org/draft/2019-09/schema",
    "type": "object",
    "title": "Dialog with Slidein Type Defs",
    "properties": {
        "agent_download": {
            "type": "object",
            "$ref": "#/$defs/agent_download"
        }
    },
    "$defs": {
        "agent_download": {
            "type": "object",
            "properties": {
                "output": { "type": "string" },
                "agent_slideout": { "$ref": "agent_slideout.json" },
                "host_name": { "type": "string" },
                "all_agents_url": {
                    "type": "string"
                }
            },
            "required": ["agent_slideout", "host_name", "all_agents_url"]
        }
    }
}
