{
	"version": "2.0.0",
	"tasks": [
		{
			"type": "cargo",
			"command": "build",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "cargo build"
		},
		{
			"type": "cargo",
			"command": "test",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "cargo test"
		},
		{
			"type": "cargo",
			"command": "clippy",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "cargo clippy"
		},
		{
			"type": "cargo",
			"command": "check",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "cargo check"
		},
		{
			"type": "shell",
			"command": "./run --all",
			"problemMatcher": [
				"$rustc"
			],
			"group": "build",
			"label": "run all"
		}
	]
}